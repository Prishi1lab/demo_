      "source": [
        "# remove punctuations\n",
        "# we remove everything except letters and spaces\n",
        "# Using re - regular expression // regex  PATTERN\n",
        "\n",
        "import re\n",
        "text = re.sub(r'[^\\w\\s]','',text)\n",
        "print(\"without punctuation:\",text)\n",
        "# remove everything except letter and spaces"
      ]
    },
