      "source": [
        "# remove stopwords i, am, is, are, the nlt all stop words present\n",
        "stopwords = [\"i\",\"am\",\"is\",\"the\",\"a\",\"an\",\"very\"]\n",
        "filtered_tokens = []\n",
        "for words in tokens:\n",
        "  if words not in stopwords:\n",
        "    filtered_tokens.append(words)\n",
        "print(\"After removing stopwords : \",filtered_tokens)"
      ]
